# Your private API key for the Lighter exchange
API_KEY_PRIVATE_KEY=your_api_key_private_key

# Your account index on the Lighter exchange
ACCOUNT_INDEX=your_account_index

# The index of the API key you are using
API_KEY_INDEX=your_api_key_index

# Max parquet file size before rotating to a new part (MB)
PARQUET_MAX_MB=5

# Symbol to use (make sure to also add it to CRYPTO_TICKERS in config.json)
MARKET_SYMBOL=PAXG
